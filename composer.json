{
    "name": "paveljurasek/aukro-api",
    "description": "Aukro.cz WebAPI client",
    "license": "MIT",
    "type": "library",
    "require": {
        "ext-openssl": "*",
        "ext-soap": "*",
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "consistence/consistence": "^0.22.0",
        "nette/utils": "^2.4"
    },
    "require-dev": {
        "nette/tester": "^1.7",
        "phpstan/phpstan": "^0.6.4",
        "mockery/mockery": "^0.9.9",
        "tracy/tracy": "^2.4",
        "nette/http": "^2.4",
        "kdyby/fake-session": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AukroApi\\": "src"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon -l5 src/ --ansi",
        "tester": "vendor/bin/tester tests/",
        "all": [
            "@phpstan",
            "@tester"
        ]
    }
}
